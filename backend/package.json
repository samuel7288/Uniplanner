    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0",
    "ics": "^3.8.1",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^8.0.1",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "swagger-ui-express": "^5.0.1",
    "web-push": "^3.6.7",
    "zod": "^3.23.8"
